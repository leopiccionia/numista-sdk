{
  "name": "@leopiccionia/numista-sdk",
  "version": "0.3.0",
  "description": "Unofficial SDK for Numista",
  "license": "MIT",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build && tsup src/index.ts --dts-only --clean",
    "docs": "typedoc --disableSources --excludeExternals --excludePrivate --githubPages false --plugin typedoc-plugin-markdown",
    "test": "vitest"
  },
  "keywords": [
    "numista"
  ],
  "author": {
    "email": "leopiccionia@gmail.com",
    "name": "Leonardo Piccioni de Almeida",
    "url": "https://github.com/leopiccionia"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leopiccionia/numista-sdk.git"
  },
  "homepage": "https://leopiccionia.github.io/numista-sdk/",
  "bugs": {
    "email": "leopiccionia@gmail.com",
    "url": "https://github.com/leopiccionia/numista-sdk/issues"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/uuid": "^8.3.4",
    "tsup": "^5.12.3",
    "typedoc": "^0.22.13",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.8.3"
  }
}
